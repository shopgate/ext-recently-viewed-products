{
  "version": "0.1.0",
  "id": "@shopgate/recently-viewed-products",
  "components": [
    {
      "id": "EmptyCart",
      "type": "portals",
      "path": "frontend/components/EmptyCart",
      "target": "cart.empty"
    },
    {
      "id": "ProductSlider",
      "type": "portals",
      "path": "frontend/components/ProductSlider",
      "target": "cart.empty.after"
    },
    {
      "id": "RecentlyViewedPage",
      "type": "portals",
      "path": "frontend/components/RecentlyViewedPage",
      "target": "page.content.after"
    },
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "productList",
      "path": "frontend/reducers/index.js",
      "type": "reducers"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    },
    {
      "type": "translations",
      "path": "frontend/locale/de-DE.json",
      "id": "locale/de-DE"
    }
  ],
  "configuration": {
    "maximumHistoryEntriesPerUser": {
      "type": "static",
      "destination": "both",
      "params": {
        "value": 50
      }
    },
    "pageId": {
      "type": "static",
      "destination": "frontend",
      "params": {
        "value": "recently-viewed"
      }
    }
  }
}
