{
  "version": "0.2.0",
  "id": "@shopgate/recently-viewed-products",
  "components": [
    {
      "id": "EmptyCart",
      "type": "portals",
      "path": "frontend/components/EmptyCart",
      "target": "cart.empty"
    },
    {
      "id": "ProductSlider",
      "type": "portals",
      "path": "frontend/components/ProductSlider",
      "target": "cart.empty.after"
    },
    {
      "id": "RecentlyViewedRoute",
      "type": "portals",
      "path": "frontend/pages/RecentlyViewed",
      "target": "app.routes"
    },
    {
      "id": "NavDrawerItemGMD",
      "path": "frontend/components/NavDrawerItem/index.jsx",
      "target": "nav-menu.cart.after",
      "type": "portals"
    },
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "productList",
      "path": "frontend/reducers/index.js",
      "type": "reducers"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    },
    {
      "type": "translations",
      "path": "frontend/locale/de-DE.json",
      "id": "locale/de-DE"
    }
  ],
  "configuration": {
    "maximumHistoryEntriesPerUser": {
      "type": "static",
      "destination": "both",
      "params": {
        "value": 50
      }
    }
  }
}
