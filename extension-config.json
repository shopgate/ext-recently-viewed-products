{
  "version": "0.0.1",
  "id": "@shopgate/recently-viewed-products",
  "components": [
    {
      "id": "RecentlyViewedProducts",
      "type": "portals",
      "path": "frontend/components/RecentlyViewedProducts",
      "target": "cart.empty.after"
    },
    {
      "id": "Subscribers",
      "path": "frontend/subscriptions/index.js",
      "type": "subscribers"
    },
    {
      "id": "productList",
      "path": "frontend/reducers/index.js",
      "type": "reducers"
    },
    {
      "type": "translations",
      "path": "frontend/locale/en-US.json",
      "id": "locale/en-US"
    },
    {
      "type": "translations",
      "path": "frontend/locale/de-DE.json",
      "id": "locale/de-DE"
    }
  ],
  "configuration": {
    "maximumHistoryEntriesPerUser": {
      "type": "static",
      "destination": "backend",
      "params": {
        "value": 50
      }
    }
  }
}
